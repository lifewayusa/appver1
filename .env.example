# API Keys for Image Download Service
# Get these keys from the respective providers:

# Unsplash (Priority #1) - https://unsplash.com/developers
# Sign up → Create Application → Copy Access Key
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Pexels (Priority #2) - https://www.pexels.com/api/
# Sign up → Generate API Key
PEXELS_API_KEY=your_pexels_api_key_here

# Pixabay (Priority #3) - https://pixabay.com/api/docs/
# Sign up → Get API Key
PIXABAY_API_KEY=your_pixabay_api_key_here

# Image Download Configuration (Optional - defaults provided)
IMAGE_QUALITY=high
IMAGE_MIN_WIDTH=1920
IMAGE_MIN_HEIGHT=1080
MAX_IMAGES_PER_CITY=1
DOWNLOAD_DELAY_MS=2000
RETRY_ATTEMPTS=3

# Existing Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_KEY=your_supabase_anon_key
