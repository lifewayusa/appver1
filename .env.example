# LifeWayUSA - Configurações de Ambiente
# Copie este arquivo para .env.local e preencha com suas credenciais

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Stripe Payment (quando implementar)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Image APIs para busca de imagens das cidades
# OBRIGATÓRIO: Pelo menos uma API deve estar configurada
PEXELS_API_KEY=your_pexels_api_key_here

# OPCIONAL: APIs adicionais para melhor performance
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here
PIXABAY_API_KEY=your_pixabay_api_key_here

# Newsletter/Email Service (quando implementar)
RESEND_API_KEY=your_resend_api_key_here

# Development
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3001
